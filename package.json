{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:renderer": "webpack -c renderer/webpack.config.js --mode production",
    "build:processor": "webpack -c processor/webpack.config.js --mode production",
    "build": "npm run build:renderer && npm run build:processor",
    "clean": "rm -rf ./build",
    "build:plugin:simple": "webpack -c plugins/simple/webpack.config.js --mode production",
    "build:plugin:nextPlayer": "webpack -c plugins/nextPlayer/webpack.config.js --mode production",
    "clean:plugins": "rm -rf ./build/plugins"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-flow": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/react": "^17.0.30",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.4.0",
    "electron": "^15.1.2",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.2.6",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "@types/node": "^18.11.12",
    "@types/react-dom": "^17.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "typescript": "^4.4.4"
  }
}